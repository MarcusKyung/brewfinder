  "version": "1.2.0",
  "description": "Chrome extension to find closest brewery/brewpub to me by location",
  "manifest_version": 3,
  "author": "Marcus Kyung",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "BrewFinder",
